    <li><a href="<%- config.root %><%- page.prev_link %>"><i class="fa fa-angle-double-left"></i> Prev</a></li>
  <%
    } else {
  %>
    <li class="disabled"><a><i class="fa fa-angle-double-left"></i> Prev</a></li>
